<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="horizontalPadding" format="dimension" /> 
    <declare-styleable name="SeekbarPreference">
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="defaultValue" format="integer" />
        <attr name="multiplier" format="float" />
        <attr name="format" format="string" />
    </declare-styleable>
    <declare-styleable name="ColorPreference">
        <attr name="defaultColor" format="color" />
    </declare-styleable>
    <declare-styleable name="FolderPickerPreference">
        <attr name="defaultFolder" format="string" />
    </declare-styleable>
    <!-- Default style attribute which will be used during construction of FloatingActionButton. -->
    <attr name="floatingActionButtonStyle" format="reference"/>
    <declare-styleable name="FloatingActionButton">
        <!-- Background color -->
        <attr name="floatingActionButtonColor" format="color|reference"/>
        <!-- Size of action button -->
        <attr name="floatingActionButtonSize" format="enum">
            <!-- 56dp -->
            <enum name="normal" value="0"/>
            <!-- 40dp -->
            <enum name="mini" value="1"/>
        </attr>
    </declare-styleable>
</resources>